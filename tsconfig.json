{
  "compilerOptions": {
    "strict": true,
    "declaration": true,
    "skipLibCheck": true,
    "esModuleInterop": true,
    "jsx": "react",
    "baseUrl": "./",
    "lib": [
      "DOM",
      "DOM.Iterable",
      "ESNext"
    ],
    "moduleResolution": "bundler",
    "module": "ESNext",
    "paths": {
      "@@/*": [
        ".dumi/tmp/*"
      ],
      "file-explorer": [
        "src"
      ],
      "file-explorer/*": [
        "src/*",
        "docs-src/*",
        "*"
      ]
    }
  },
  "include": [
    ".dumirc.ts",
    "src/**/*",
    "docs-src/**/*"
  ]
}